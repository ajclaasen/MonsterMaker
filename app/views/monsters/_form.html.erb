<%= simple_form_for @monster do |f| %>
  <%= f.input :name %>
  <%= f.input :sizeTypeAlignment %>
  <%= f.input :armorClass %>
  <%= f.input :hitPointsAndDice %>
  <%= f.input :speed %>
  <%= f.input :strength %>
  <%= f.input :dexterity %>
  <%= f.input :constitution %>
  <%= f.input :intelligence %>
  <%= f.input :wisdom %>
  <%= f.input :charisma %>
  <%= f.input :skills %>
  <%= f.input :damageImmunities %>
  <%= f.input :conditionImmunities %>
  <%= f.input :senses %>
  <%= f.input :languages %>
  <%= f.input :challenge %>
  <%= f.button :submit %>
<% end %>